<?xml version="1.0" encoding="utf-8"?>

<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".isinotes">>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageButton
            android:id="@+id/buttonback"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="43dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="320dp"
            android:backgroundTint="#00FFFFFF"
            android:scaleType="centerCrop"
            android:src="@drawable/back" />

        <TextView
            android:id="@+id/textView19"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_alignStart="@id/buttonback"
            android:layout_alignTop="@id/buttonback"
            android:layout_marginStart="50dp"
            android:layout_marginTop="10dp"
            android:text="MY NOTES"
            android:textColor="#000000"
            android:textSize="18sp" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@id/buttonback"
            android:layout_alignTop="@id/buttonback"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="0dp"
            android:layout_marginTop="80dp"
            android:layout_marginEnd="40dp"/>

        <ImageView
            android:id="@+id/rectangle_isinotes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@id/buttonback"
            android:layout_alignTop="@id/buttonback"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="0dp"
            android:layout_marginTop="170dp"
            android:layout_marginEnd="40dp"
            app:srcCompat="@drawable/rectangle_isinotes" />

        <ImageView
            android:id="@+id/judulnotes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@id/buttonback"
            android:layout_alignTop="@id/buttonback"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="0dp"
            android:layout_marginTop="80dp"
            android:layout_marginEnd="40dp"
            app:srcCompat="@drawable/rectangle_judulnotes" />

        <ImageButton
            android:id="@+id/btn_edit"
            android:layout_width="139dp"
            android:layout_height="wrap_content"
            android:layout_alignStart="@id/buttonback"
            android:layout_alignTop="@id/rectangle_isinotes"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="189dp"
            android:layout_marginTop="462dp"
            android:layout_marginEnd="40dp"
            android:backgroundTint="#00FFFFFF"

            app:srcCompat="@drawable/btn_edit" />

        <ImageButton
            android:id="@+id/btn_delete"
            android:layout_width="wrap_content"
            android:layout_height="57dp"
            android:layout_alignStart="@id/buttonback"
            android:layout_alignTop="@id/rectangle_isinotes"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="2dp"
            android:layout_marginTop="466dp"
            android:layout_marginEnd="218dp"
            android:backgroundTint="#00FFFFFF"
            app:srcCompat="@drawable/btn_delete" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@id/buttonback"
            android:layout_alignTop="@id/buttonback"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="10dp"
            android:layout_marginTop="100dp"
            android:layout_marginEnd="40dp"
            android:text="JUDUL"
            android:textSize="20dp" />
        <TextView
            android:id="@+id/textnotes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@id/buttonback"
            android:layout_alignTop="@id/buttonback"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="10dp"
            android:layout_marginTop="190dp"
            android:layout_marginEnd="40dp"
            android:text="isinotes"
            android:textSize="15dp" />


    </RelativeLayout>
</ScrollView>